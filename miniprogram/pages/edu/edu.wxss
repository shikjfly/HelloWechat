  border-right: none;
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  box-sizing: border-box;
}
.list-title{
  margin: 20rpx;
  color: black;
  font-size: 28px;
  font-style: bold;
  display: block;
}
.list-item{
  width: 100%;
  padding: 10rpx;
  font-size: 16rpx;
  color: #007aff;
  border-top: 1px solid rgba(0, 0, 0, 0.1);
  box-sizing: border-box;
}
.list-text{
  color: red;
  font-size: 12px;
  display: block;
}
